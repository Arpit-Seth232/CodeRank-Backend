{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/CodeRank-Backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/CodeRank-Backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/CodeRank-Backend",
  "projects": {
    "CodeRank-Backend": {
      "type": "application",
      "root": "apps/CodeRank-Backend",
      "entryFile": "main",
      "sourceRoot": "apps/CodeRank-Backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/CodeRank-Backend/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    }
  }
}